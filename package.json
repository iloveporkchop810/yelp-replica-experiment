    "faker": "^4.1.0",
    "jquery": "^3.3.1",
    "moment": "^2.23.0",
    "mysql": "^2.16.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
