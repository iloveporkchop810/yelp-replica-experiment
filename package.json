    "file-loader": "^2.0.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^22.3.0",
    "morgan": "^1.9.0",
    "prettier": "1.14.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^2.2.1"
  },
